{
  "name": "infra",
  "version": "0.1.0",
  "description": "CDKのサンプルアプリケーションです。",
  "license": "UNLICENSED",
  "private": true,
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --config .prettierrc --write .",
    "typecheck": "tsc --skipLibCheck true --noEmit",
    "typecheck:full": "tsc --skipLibCheck false --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "aws-cdk-lib": "2.201.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/jest": "^29.5.14",
    "@types/node": "24.0.3",
    "@vitest/eslint-plugin": "^1.2.7",
    "aws-cdk": "2.1018.1",
    "eslint": "^9.29.0",
    "eslint-cdk-plugin": "^3.0.3",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.3"
  }
}
